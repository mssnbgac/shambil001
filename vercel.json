{
  "version": 2,
  "name": "shambil-pride-academy",
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "client/build/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://shambil001.onrender.com/api"
  }
}